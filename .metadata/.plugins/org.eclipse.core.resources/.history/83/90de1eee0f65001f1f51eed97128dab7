package edu.kh.variable.practice;

public class CastingPractice1 {
	
	public static void main(String[] args) {
		int iNum1 = 10;
		int iNum2 = 4;
		
		float fNum = 3.0f;
		double dNum = 2.5;
		
		
		char ch = 'A';
		int i = ch;
		
		System.out.println(ch);
		System.out.println(i);
		System.out.println(ch + 10);
		
		
		
		
		

	}
	
}
